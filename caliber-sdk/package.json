{
  "name": "@caliber-run/sdk",
  "version": "0.4.4",
  "description": "TypeScript SDK for CALIBER - Cognitive Agent Long-term Intelligence, Behavioral Episodic Recall",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./websocket": {
      "types": "./dist/websocket.d.ts",
      "import": "./dist/websocket.js"
    },
    "./context": {
      "types": "./dist/context.d.ts",
      "import": "./dist/context.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/websocket.ts src/context.ts --format esm,cjs --dts --clean",
    "dev": "tsup src/index.ts src/websocket.ts src/context.ts --format esm,cjs --dts --watch",
    "typecheck": "tsc --noEmit",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "lint": "bunx biome lint src/",
    "lint:fix": "bunx biome lint --write src/",
    "format": "bunx biome format --write src/",
    "bench": "bun run bench/index.ts",
    "bench:ci": "bun run bench/index.ts --json > bench-results.json",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20.11.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "keywords": [
    "caliber",
    "ai",
    "agent",
    "memory",
    "llm",
    "context",
    "hierarchical",
    "typescript",
    "sdk"
  ],
  "author": "Eassa Ayoub <hello@heyoub.dev>",
  "license": "AGPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/caliber-run/caliber"
  },
  "bugs": {
    "url": "https://github.com/caliber-run/caliber/issues"
  },
  "homepage": "https://caliber.run",
  "packageManager": "bun@1.1.0",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
